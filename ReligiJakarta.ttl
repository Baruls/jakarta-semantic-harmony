@prefix : <http://www.semanticweb.org/religion/jakarta#> .
@prefix rj: <http://www.semanticweb.org/religion/jakarta#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@base <http://www.semanticweb.org/religion/jakarta> .

<http://www.semanticweb.org/religion/jakarta> rdf:type owl:Ontology .

#################################################################
#    1. CLASSES
#################################################################
rj:Place a owl:Class .
rj:ReligiousSite a owl:Class ; rdfs:subClassOf rj:Place .
rj:HeritageSite a owl:Class ; rdfs:subClassOf rj:ReligiousSite .
rj:Mosque a owl:Class ; rdfs:subClassOf rj:ReligiousSite .
rj:Church a owl:Class ; rdfs:subClassOf rj:ReligiousSite .
rj:Vihara a owl:Class ; rdfs:subClassOf rj:ReligiousSite .
rj:Temple a owl:Class ; rdfs:subClassOf rj:ReligiousSite .
rj:TransportNode a owl:Class .
rj:TrainStation a owl:Class ; rdfs:subClassOf rj:TransportNode .
rj:BusStop a owl:Class ; rdfs:subClassOf rj:TransportNode .
rj:City a owl:Class . rj:District a owl:Class . rj:Religion a owl:Class .

#################################################################
#    2. PROPERTIES
#################################################################
rj:nearestTransport a owl:ObjectProperty .
rj:locatedInDistrict a owl:ObjectProperty .
rj:hasLocation a owl:ObjectProperty .
rj:practicesReligion a owl:ObjectProperty .
schema:name a owl:DatatypeProperty .
schema:streetAddress a owl:DatatypeProperty . 
schema:openingHours a owl:DatatypeProperty .
rj:postalCode a owl:DatatypeProperty .       
rj:capacity a owl:DatatypeProperty .         
rj:surfaceArea a owl:DatatypeProperty .      
rj:architect a owl:DatatypeProperty .        
rj:heritageCode a owl:DatatypeProperty .
rj:isHeritageSite a owl:DatatypeProperty .
rj:mosqueType a owl:DatatypeProperty .
rj:establishedYear a owl:DatatypeProperty .

#################################################################
#    3. INDIVIDUALS (DATA LENGKAP)
#################################################################

