rj:MasjidIstiqlal a owl:NamedIndividual, rj:Mosque, rj:HeritageSite ;
    schema:name "MASJID ISTIQLAL"@id ;
    schema:streetAddress "Jl. Taman Wijaya Kusuma No. 1"@id ;
    # --- PROPERTI WAJIB UNTUK APP.PY ---
    schema:openingHours "04:00 - 22:00 WIB" ; 
    rj:postalCode "10710" ;
    rj:hasLocation rj:JakartaPusat ;
    rj:locatedInDistrict rj:KecamatanSawahBesar ;
    rj:capacity "200000"^^xsd:integer ;
    rj:surfaceArea "9.5 Hektar" ;
    rj:architect "Friedrich Silaban" ;
    rj:mosqueType "Masjid Negara" ;
    rj:isHeritageSite "true"^^xsd:boolean ;
    rj:nearestTransport rj:StasiunJuanda ;
    rj:practicesReligion rj:Islam .

